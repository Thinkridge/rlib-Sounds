{
  "name": "@thinkridge/rlib-soundfont",
  "version": "0.0.2",
  "publishConfig": {
    "access": "public"
  },
  "description": "SoundFont decoder",
  "keywords": [
    "soundfont",
    "sf2",
    "midi",
    "wasm",
    "audio"
  ],
  "engines": {
    "node": ">=18"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "types": "./dist/index.d.ts",
  "scripts": {
    "build:wasm": "docker run --rm -v ./../src:/opt/vol -w /opt/vol emsdk \"cd wasm && mkdir -p build && cd build && emcmake cmake .. && make\"",
    "build": "npm run build:wasm && tsup"
  },
  "devDependencies": {
    "tsup": "^8.5.1",
    "typescript": "^5.4.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/thinkridge/rlib-Sounds"
  },
  "bugs": {
    "url": "https://github.com/thinkridge/rlib-Sounds/issues"
  },
  "homepage": "https://github.com/thinkridge/rlib-Sounds"
}
